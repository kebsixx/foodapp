{
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enablePaths": ["supabase/functions"],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "deno.enable": true,
  "chat.mcp.serverSampling": {
    "foodApp/.vscode/mcp.json: supabase": {
      "allowedModels": [
        "github.copilot-chat/gpt-4.1",
        "github.copilot-chat/claude-3.5-sonnet",
        "github.copilot-chat/Ollama-deepseek-r1",
        "github.copilot-chat/gemini-2.0-flash-001",
        "github.copilot-chat/gpt-4o",
        "github.copilot-chat/o3-mini"
      ]
    }
  }
}
