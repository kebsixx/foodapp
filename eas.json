{
  "cli": {
    "version": ">= 14.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "channel": "staging",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "gradleCommand": ":app:clean :app:bundleRelease",
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
